programa
{
	
	funcao inicio()
	{
		real peso_dos_peixes
		real excesso
		real multa

		escreva("Digite o peso de peixes ")
		leia(peso_dos_peixes)
		
		excesso= peso_dos_peixes- 50
		se (excesso< 0)
		excesso = 0
		multa = excesso * 4.50

		escreva("Peso total pescado: ", peso_dos_peixes, " kg.\n")
		escreva("Excesso de peso: ", excesso, " kg.\n")
		escreva("Multa a ser paga: R$ ", multa)
		

	}
}
