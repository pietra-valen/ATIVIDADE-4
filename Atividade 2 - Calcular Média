programa
{
	
	funcao inicio()
	{
		real n1 , n2 , n3 , n4 , media

		escreva ("Digite a nota 1 ")
		leia (n1)

		escreva ("Digite a nota 2 ")
		leia (n2)

		escreva ("Digite a nota 3 ")
		leia (n3)

		escreva ("Digite a nota 4 ")
		leia (n4)

		media = (n1 + n2 + n3 + n4) / 4

		escreva ("Sua média é " , media)
	}
}
